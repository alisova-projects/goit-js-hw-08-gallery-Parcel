{
  "name": "goit-js-hw-08-gallery-parcel",
  "version": "1.0.0",
  "description": "Gallery in Parcel",
  "main": "index.js",
  "dependencies": {
    "gh-pages": "^3.1.0",
    "parcel-bundler": "^1.12.4"
  },
  "devDependencies": {
    "parcel-plugin-custom-dist-structure": "^1.1.16",
    "sass": "^1.26.11"
  },
  "scripts": {
    "dev": "parcel src/index.html",
    "build": "parcel build src/index.html",
    "predeploy": "npm run build -- --public-url ./goit-js-hw-08-gallery-Parcel/",
    "deploy": "gh-pages -d dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alisova-projects/goit-js-hw-08-gallery-Parcel.git"
  },
  "customDistStructure": {
    "config": {
      ".css": "css",
      ".js": "js",
      "images": [
        ".jpg",
        ".svg",
        ".png"
      ]
    }
  },
  "author": "Elena Alisova",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/alisova-projects/goit-js-hw-08-gallery-Parcel/issues"
  },
  "homepage": "https://alisova-projects.github.io/goit-js-hw-08-gallery-Parcel"
}
